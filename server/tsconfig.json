{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS", // Node.js 환경에 적합
      "moduleResolution": "Node",
      "outDir": "dist",
      // "rootDir": "./src",
      "esModuleInterop": true,
      "strict": true,
      "types": ["node"],
      "baseUrl": "./",
      "skipLibCheck": true
    },
    "include": ["**/*", "*.ts"],
    "exclude": ["node_modules", "dist"] //dist: 빌드 출력 폴더
  }
  